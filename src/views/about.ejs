<!doctype html>
<html class="no-js" lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <%- include('partials/head.ejs', { data: { title: 'Who We Are' }}) %>
</head>

<body>

  <div class="wrapper">

    <%- include('partials/header.ejs', { data: { state: 'about' }}) %>

    <section class="bg-faded py-5">
      <div class="container pt-5">
        <div class="row">
          <div class="col-md-12 text-center">
            <div class="what-top meet">
              <div class="section-title">
                <h1 class="title-font">Who We Are</h1>
                <div class="what-icon">
                  <i class="fa fa-users" aria-hidden="true"></i>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="row justify-content-md-center mb-5">
          <div class="col-12 col-md-10">
            <component content></component>
          </div>
        </div>

        <div class="row justify-content-md-center mb-5 pb-5">
          <div class="col-12 col-md-10 text-center">
            <h2 class="title-font">Leadership</h2>
            <component>
              <div v-for="item in items" class="m-3" style="display: inline-block; max-width: 160px; vertical-align: top;">
                <img class="rounded-circle m-2" src="https://placeholdit.imgix.net/~text?txtsize=18&txt=Fight%20For%20Better&w=120&h=120" />
                <h5 v-text="item.name" class="mb-1"></h5>
                <p v-text="item.role"></p>
              </div>
            </component>
            <strong>...plus hundreds of colleagues & friends</strong>
            <br>
            *founding team
          </div>
        </div>

      </div>
    </section>

    <%- include('partials/footer.ejs') %>
  </div>

  <%- include('partials/scripts.ejs') %>

</body>

</html>
