<!doctype html>
<html class="no-js" lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="x-ua-compatible" content="ie=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <%- include('partials/head.ejs', { data: { title: 'Who We Are' }}) %>
</head>

<body>

  <div class="wrapper">

    <%- include('partials/header.ejs', { data: { state: 'about' }}) %>

    <section class="bg-faded py-5" style="min-height: 400px;">
      <div class="container">
        <div class="row">
          <div class="col-md-12">
            <div class="card mb-5 p-4">
              <div class="card-block">

                <div class="what-top meet text-center">
                  <div class="section-title">
                    <h1 class="title-font">Who We Are</h1>
                    <div class="what-icon">
                      <i class="fa fa-users" aria-hidden="true"></i>
                    </div>
                  </div>
                </div>

                <div class="loader" component-loading></div>

                <div component-cloak class="row justify-content-md-center mb-5">
                  <div class="col-12 col-md-10">
                    <component key=ebrdn></component>
                  </div>
                </div>

                <div component-cloak class="row justify-content-md-center mb-5 pb-5">
                  <div class="col-12 col-md-10 text-center">
                    <h2 class="title-font">Leadership</h2>
                    <component key=emnon></component>
                    <strong>...plus hundreds of colleagues & friends</strong>
                    <br>
                    *founding team
                  </div>
                </div>

              </div>
            </div>

          </div>
        </div>
      </div>
    </section>

    <%- include('partials/footer.ejs') %>
  </div>

  <%- include('partials/scripts.ejs') %>

</body>

</html>
